<script language="JavaScript" type="text/javascript">
window.__starSupport={
    defaultMessage: "Welcome to Star Support! This tool lets our support agents guide you right from your browser. <br/><br/>To use, call customer service at +1.866.788.9225 and tell them your Star Support number is <span class='notranslate star_support_code'>X2936</span>.",
    errorMessage: "An unknown error occurred. Please reload the page and try again.",
    defaultTimeoutMessage: "Your connection has been dropped due to inactivity. Click here to reconnect.",
    connect: function() {
        window.__starSupport.starDisplayFeedback(window.__starSupport.defaultMessage, 'connecting');
        window.__starSupport.transmit('X2936','visit_d41d8cd98f00b20_4d73fa270e85e2e','0','-1');
    },
    transmit: function (code,visit_identifier,logged_in,member_id,message) {
        if (typeof(message)=='undefined') message=false;
        plugins="";
        for(i=0;i<navigator.plugins.length;i++) plugins+=(i+1)+") "+navigator.plugins[i].name+"<br/>";
        MimeTypes="";
        for(i=0;i<navigator.mimeTypes.length;i++) {
            MimeTypes+=(i+1)+") "+navigator.mimeTypes[i].description+"<br/>\n";
            for (j=0;j<navigator.mimeTypes[i].enabledPlugin.length;j++) MimeTypes+="&nbsp;&nbsp;&nbsp;&nbsp;- Type: "+navigator.mimeTypes[i].enabledPlugin[j].suffixes+" ("+navigator.mimeTypes[i].enabledPlugin[j].type+")<br/>\n";
        }
        browser_data={Browser:__starSupport.getBrowserName(),OperatingSystem:__starSupport.getOSName(), UserAgent:navigator.userAgent,Language:navigator.language,
            CookiesEnabled:navigator.cookieEnabled, Platform:navigator.platform,
            Height:$(document).height(),Width:$(document).width(),Plugins:plugins,MimeTypes:MimeTypes};
        if (message==true) {
            message=$('#star_support_message_content').val();
            task='send_message';
        } else {
            task='click';
            message=0;
        }
        data={action:task,code:code,visit_identifier:visit_identifier,logged_in:logged_in,member_id:member_id,
            client_url:window.location.href,
            referrer:'',
            browser_data:browser_data,
            dom:document.documentElement.outerHTML,
            message:message};
        $.ajax({type:'post', url:'/inc/star_support.php', data:data}).done(window.__starSupport.processStarResponse);
        window.__starSupport.eraseCookie('_ff_star_hide');
    },
    openMessageWindow: function (code,visit_identifier,logged_in,member_id) {
        content="Message:<br/><textarea id=star_support_message_content rows=5 cols=48></textarea><br/><button id=star_send_message_button >Send</button>";
        $('#star_holder2 #star_feedback').html(content);
        $('#star_holder2 #star_send_message_button').bind('click',function(){
            window.__starSupport.transmit('X2936','visit_d41d8cd98f00b20_4d73fa270e85e2e','0','-1',true);
            window.__starSupport.starDisplayFeedback('Message sent.');
        });
    },
    starPinger: function (code, start) {
        timeout=300; //sec
        interval=2; //sec
        if (typeof(start)=='undefined') start=1;
        if (start) __starSupport.starPingStart=new Date().getTime()/1000;
        curr_time=new Date().getTime()/1000;
        if (curr_time-__starSupport.starPingStart<=timeout) {
            setTimeout(function() {$.ajax({type:'post', url:'/inc/star_support.php', data:{action:'ping',code:code}}).done(__starSupport.processStarResponse);},interval*1000);
            return true;
        } else {
            window.__starSupport.starDisplayFeedback(window.__starSupport.defaultTimeoutMessage,'reconnect');
            return false;
        }
    },
    processStarResponse: function (response) {
        response=jQuery.parseJSON(response);
        if (response.error==1) {__starSupport.starDisplayFeedback(window.__starSupport.errorMessage,'error');return false;}
        else if (response.action=='show_code') {
            __starSupport.starPinger(response.code);
            return true;
        } else if (response.action=='ping') {
            __starSupport.starPinger(response.code,0);
        } else if (response.action=='push') {
            action_data=response.data;
            if (action_data.do=='redirect') {
                if (confirm("You are being redirected by a customer service representative.")) {
                    window.location.href=action_data.url;
                    __starSupport.starDisplayFeedback('You are being redirected by a customer service representative.','redirect');
                    return true;
                } else {
                    __starSupport.starDisplayFeedback('Redirection canceled.','connected');
                    __starSupport.starPinger(response.code);
                    return false;
                }
            } else if (action_data.do=='message') {
                __starSupport.starDisplayFeedback('Message from Support Agent:<br/><b>'+action_data.message+'</b>');
                __starSupport.starPinger(response.code);
            } else {__starSupport.starFailSafe();return false;}
        } else {
            __starSupport.starFailSafe(); return false;
        }
    },
    starDisplayFeedback: function(content, status) {
        if (typeof(status)==='undefined') status='connected';
        if (!$('#star_holder2').is(':visible')) $('#star_holder2').fadeIn(500);
        if (status=='reconnect') {
            $('#star_holder2 #star_feedback').html(content);
            $('#star_holder2 #star_feedback').bind(('click'),function() {window.__starSupport.connect();});
            $('#star_holder2 #star_feedback').css('cursor','pointer');
        } else {
            $('#star_holder2 #star_feedback').unbind('click');
            $('#star_holder2 #star_feedback').html(content);
            $('#star_holder2 #star_feedback').css('cursor','auto');
        }
    },
    starFailSafe: function () {__starSupport.starDisplayFeedback(window.__starSupport.errorMessage,'error');},
    setCookie: function (name, value, minutes) {
        var expires;
        if (minutes) {
            var date = new Date();
            date.setTime(date.getTime() + (minutes * 1000));
            expires = "; expires=" + date.toGMTString();
        } else expires = "";
        document.cookie = encodeURI(name) + "=" + encodeURI(value) + expires + "; path=/";
    },
    readCookie: function (name) {
        var nameEQ = encodeURI(name) + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) === ' ') c = c.substring(1, c.length);
            if (c.indexOf(nameEQ) === 0) return encodeURI(c.substring(nameEQ.length, c.length));
        }
        return null;
    },
    eraseCookie: function (name) {this.setCookie(name, "", -1);},
    getBrowserName: function () {
        var nVer = navigator.appVersion;
        var nAgt = navigator.userAgent;
        var browserName  = navigator.appName;
        var fullVersion  = ''+parseFloat(navigator.appVersion);
        var majorVersion = parseInt(navigator.appVersion,10);
        var nameOffset,verOffset,ix;

        // In Opera, the true version is after "Opera" or after "Version"
        if ((verOffset=nAgt.indexOf("Opera"))!=-1) {
            browserName = "Opera";
            fullVersion = nAgt.substring(verOffset+6);
            if ((verOffset=nAgt.indexOf("Version"))!=-1)
                fullVersion = nAgt.substring(verOffset+8);
        }
        // In older MSIE, the true version is after "MSIE" in userAgent
        else if ((verOffset=nAgt.indexOf("MSIE"))!=-1) {
            browserName = "Microsoft Internet Explorer";
            fullVersion = nAgt.substring(verOffset+5);
        }
        // In newer MSIE, the true version is after "MSIE" in userAgent
        else if ((verOffset=nAgt.indexOf(".NET"))!=-1) {
            browserName = "Microsoft Internet Explorer";
            var nVer = navigator.appVersion;
            var nAgt = navigator.userAgent;
            var browserName  = navigator.appName;
            var fullVersion  = ''+parseFloat(navigator.appVersion);
            var majorVersion = parseInt(navigator.appVersion,10);
            var nameOffset,verOffset,ix;
            // In Opera, the true version is after "Opera" or after "Version"
            if ((verOffset=nAgt.indexOf("Opera"))!=-1) {
                browserName = "Opera";
                fullVersion = nAgt.substring(verOffset+6);
                if ((verOffset=nAgt.indexOf("Version"))!=-1) fullVersion = nAgt.substring(verOffset+8);
            }
            // In older MSIE, the true version is after "MSIE" in userAgent
            else if ((verOffset=nAgt.indexOf("MSIE"))!=-1) {
                browserName = "Microsoft Internet Explorer";
                fullVersion = nAgt.substring(verOffset+5);
            }
            // In newer MSIE, the true version is after "MSIE" in userAgent
            else if ((verOffset=nAgt.indexOf(".NET"))!=-1) {
                browserName = "Microsoft Internet Explorer";
                fullVersion = nAgt.substring(nAgt.indexOf('rv:')+3);
            }
            // In Chrome, the true version is after "Chrome"
            else if ((verOffset=nAgt.indexOf("Chrome"))!=-1) {
                browserName = "Chrome";
                fullVersion = nAgt.substring(verOffset+7);
            }
            // In Safari, the true version is after "Safari" or after "Version"
            else if ((verOffset=nAgt.indexOf("Safari"))!=-1) {
                browserName = "Safari";
                fullVersion = nAgt.substring(verOffset+7);
                if ((verOffset=nAgt.indexOf("Version"))!=-1) fullVersion = nAgt.substring(verOffset+8);
            }
            // In Firefox, the true version is after "Firefox"
            else if ((verOffset=nAgt.indexOf("Firefox"))!=-1) {
                browserName = "Firefox";
                fullVersion = nAgt.substring(verOffset+8);
            }
            // In most other browsers, "name/version" is at the end of userAgent
            else if ( (nameOffset=nAgt.lastIndexOf(' ')+1) < (verOffset=nAgt.lastIndexOf('/')) ) {
                browserName = nAgt.substring(nameOffset,verOffset);
                fullVersion = nAgt.substring(verOffset+1);
                if (browserName.toLowerCase()==browserName.toUpperCase()) browserName = navigator.appName;
            }
            // trim the fullVersion string at semicolon/space if present
            if ((ix=fullVersion.indexOf(";"))!=-1) fullVersion=fullVersion.substring(0,ix);
            if ((ix=fullVersion.indexOf(" "))!=-1) fullVersion=fullVersion.substring(0,ix);
            if ((ix=fullVersion.indexOf(")"))!=-1) fullVersion=fullVersion.substring(0,ix);
            majorVersion = parseInt(''+fullVersion,10);
            if (isNaN(majorVersion)) {
                fullVersion  = ''+parseFloat(navigator.appVersion);
                majorVersion = parseInt(navigator.appVersion,10);
            }
            return browserName+" v"+fullVersion;
        }
        // In Chrome, the true version is after "Chrome"
        else if ((verOffset=nAgt.indexOf("Chrome"))!=-1) {
            browserName = "Chrome";
            fullVersion = nAgt.substring(verOffset+7);
        }
        // In Safari, the true version is after "Safari" or after "Version"
        else if ((verOffset=nAgt.indexOf("Safari"))!=-1) {
            browserName = "Safari";
            fullVersion = nAgt.substring(verOffset+7);
            if ((verOffset=nAgt.indexOf("Version"))!=-1) fullVersion = nAgt.substring(verOffset+8);
        }
        // In Firefox, the true version is after "Firefox"
        else if ((verOffset=nAgt.indexOf("Firefox"))!=-1) {
            browserName = "Firefox";
            fullVersion = nAgt.substring(verOffset+8);
        }
        // In most other browsers, "name/version" is at the end of userAgent
        else if ( (nameOffset=nAgt.lastIndexOf(' ')+1) < (verOffset=nAgt.lastIndexOf('/')) ) {
            browserName = nAgt.substring(nameOffset,verOffset);
            fullVersion = nAgt.substring(verOffset+1);
            if (browserName.toLowerCase()==browserName.toUpperCase()) {
                browserName = navigator.appName;
            }
        }
        // trim the fullVersion string at semicolon/space if present
        if ((ix=fullVersion.indexOf(";"))!=-1) fullVersion=fullVersion.substring(0,ix);
        if ((ix=fullVersion.indexOf(" "))!=-1) fullVersion=fullVersion.substring(0,ix);
        if ((ix=fullVersion.indexOf(")"))!=-1) fullVersion=fullVersion.substring(0,ix);
        majorVersion = parseInt(''+fullVersion,10);
        if (isNaN(majorVersion)) {
            fullVersion  = ''+parseFloat(navigator.appVersion);
            majorVersion = parseInt(navigator.appVersion,10);
        }
        return browserName+" v"+fullVersion;
    },
    getOSName: function () {
        var OSName="Unknown OS";
        if (navigator.appVersion.indexOf("Win")!=-1) OSName="Windows";
        if (navigator.appVersion.indexOf("Mac")!=-1) OSName="MacOS";
        if (navigator.appVersion.indexOf("X11")!=-1) OSName="UNIX";
        if (navigator.appVersion.indexOf("Linux")!=-1) OSName="Linux";
        return OSName;
    }
}
